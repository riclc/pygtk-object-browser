<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy toplevel-contextual -->
  <object class="GtkListStore" id="storeObjetos">
    <columns>
      <!-- column-name Icone -->
      <column type="GdkPixbuf"/>
      <!-- column-name Nome -->
      <column type="gchararray"/>
    </columns>
  </object>
  <object class="GtkListStore" id="storeMembros">
    <columns>
      <!-- column-name Estrela -->
      <column type="GdkPixbuf"/>
      <!-- column-name Icone -->
      <column type="GdkPixbuf"/>
      <!-- column-name Membro -->
      <column type="gchararray"/>
      <!-- column-name Tipo -->
      <column type="gchararray"/>
    </columns>
  </object>
